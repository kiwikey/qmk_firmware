{
    "manufacturer": "KiwiKey",
    "keyboard_name": "Kawii9 Rev.1",
    "maintainer": "KiwiKey",
    "url": "https://kiwikey.vn/mechkey/kawii9/",
    "bootloader": "atmel-dfu",
    "processor": "atmega32u4",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0303",
        "vid": "0x4B57",
        "no_startup_check": true
    },
    "features": {
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgblight": true
    },
    "bootmagic": {
        "enabled": true,
        "matrix": [0, 0]
    },
    "debounce": 5,
    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "cols": [ "F4", "F5", "F6" ],
        "rows": [ "B6", "B5", "B4" ]
    },
    "ws2812": {
        "pin": "D3"
    },
    "rgblight": {
        "led_count": 4,
        "max_brightness": 255,
        "brightness_steps": 8,
        "sleep": true,
        "animations": {
            "alternating": true,
            "breathing": true,
            "christmas": true,
            "knight": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "rgb_test": true,
            "snake": true,
            "static_gradient": true,
            "twinkle": true
        }
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [0, 0], "x": 0, "y": 0 },
                { "matrix": [0, 1], "x": 1, "y": 0 },
                { "matrix": [0, 2], "x": 2, "y": 0 },
                { "matrix": [1, 0], "x": 0, "y": 1 },
                { "matrix": [1, 1], "x": 1, "y": 1 },
                { "matrix": [1, 2], "x": 2, "y": 1 },
                { "matrix": [2, 0], "x": 0, "y": 2 },
                { "matrix": [2, 1], "x": 1, "y": 2 },
                { "matrix": [2, 2], "x": 2, "y": 2 }
            ]
        }
    }
}
