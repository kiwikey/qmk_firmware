{
    "keyboard_name": "Kiwi5x5",
    "manufacturer": "KiwiKey",
    "url": "kiwikey.vn",
    "maintainer": "kiwikey",
    "usb": {
        "vid": "0x4B57",
        "pid": "0x0505",
        "device_version": "1.0.0"
    },
    "features": {
		"bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "backlight": true,
        "rgblight": true,
        "nkro": true,
        "quantum_painter": true
    },
    "backlight": {
        "pin": "A1",
        "driver": "pwm",
        "breathing": false,
        "max_brightness": 255,
        "levels": 10
    }
    "ws2812": {
        "pin": "C14"
    },
    "rgblight": {
        "led_count": 35,
		"driver": "ws2812",
        "brightness_steps": 17,
        "sleep": true,
        "animations": {
            "alternating": true,
            "breathing": true,
            "christmas": true,
            "knight": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "rgb_test": true,
            "snake": true,
            "static_gradient": true,
            "twinkle": true
        }
    },
    "matrix_pins": {
        "cols": [ "B10", "B1", "B0", "A8", "A15", "C15" ],
        "rows": [ "B8",  "B7", "B3", "B4", "B6"  ]
    },
    "diode_direction": "COL2ROW",
    "encoder": {
        "enabled": true,
        "rotary": [
            {"pin_a": "A3", "pin_b": "A2"}
        ]
    },
    "layouts": {
        "LAYOUT_ortho_5x5": {
            "layout": [
				{ "matrix": [0, 0], "x": 0, "y": 0 },
				{ "matrix": [0, 1], "x": 0, "y": 1 },
				{ "matrix": [0, 2], "x": 0, "y": 2 },
				{ "matrix": [0, 3], "x": 0, "y": 3 },
				{ "matrix": [0, 4], "x": 0, "y": 4 },

				{ "matrix": [1, 0], "x": 1, "y": 0 },
				{ "matrix": [1, 1], "x": 1, "y": 1 },
				{ "matrix": [1, 2], "x": 1, "y": 2 },
				{ "matrix": [1, 3], "x": 1, "y": 3 },
				{ "matrix": [1, 4], "x": 1, "y": 4 },

				{ "matrix": [2, 0], "x": 2, "y": 0 },
				{ "matrix": [2, 1], "x": 2, "y": 1 },
				{ "matrix": [2, 2], "x": 2, "y": 2 },
				{ "matrix": [2, 3], "x": 2, "y": 3 },
				{ "matrix": [2, 4], "x": 2, "y": 4 },

				{ "matrix": [3, 0], "x": 3, "y": 0 },
				{ "matrix": [3, 1], "x": 3, "y": 1 },
				{ "matrix": [3, 2], "x": 3, "y": 2 },
				{ "matrix": [3, 3], "x": 3, "y": 3 },
				{ "matrix": [3, 4], "x": 3, "y": 4 },
				
				{ "matrix": [4, 0], "x": 4, "y": 0 },
				{ "matrix": [4, 1], "x": 4, "y": 1 },
				{ "matrix": [4, 2], "x": 4, "y": 2 },
				{ "matrix": [4, 3], "x": 4, "y": 3 },
				{ "matrix": [4, 4], "x": 4, "y": 4 },
				{ "matrix": [4, 5], "x": 4, "y": 5 }
            ]
        }
    }
